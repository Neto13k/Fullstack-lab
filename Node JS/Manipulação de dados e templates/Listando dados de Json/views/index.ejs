<!DOCTYPE html>
<html>
<head>
    <title>Usuários</title>
</head>
<body>

    <h1>Lista de Usuários</h1>

    <ul>

        <% usuarios.forEach(usuario => { %>

            <li>
                <strong>Nome:</strong> <%= usuario.nome %> <br>
                <strong>Email:</strong> <%= usuario.email %>
            </li>

            <br>

        <% }) %>

    </ul>

</body>
</html>